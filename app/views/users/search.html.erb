<% title "Search" %>
<%= render :partial => "form1"%>
<%unless @results.nil?%>
<% @results.each do |result|%>
<ul>
	  <% targets = result[0]%>
	  <% chain = result[1]%>
	<li>
	     <strong>Found</strong>
		<%targets.each do |target|%>
		
		<%= " #{target.first_name} #{target.last_name}, "%>
	    <%end%>
	   	<%chain.each do |link|%>
	<strong> Through: </strong>
		<%= "#{link.username} "%>
		<%end%>
	    
	</li>
	
</ul>
<%end%>
<%end%>

